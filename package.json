{
  "name": "sap_fiori_catalogs",
  "version": "0.0.1",
  "description": "SAP Fiori Web Application for Catalogs Module",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^3.0.0",
    "@sap/ux-ui5-tooling": "1",
    "eslint": "8.57.0",
    "@sap/eslint-plugin-ui5-jsdocs": "2.0.5",
    "@sapui5/ts-types": "~1.132.0",
    "@sap-ux/eslint-plugin-fiori-tools": "^0.4.0",
    "eslint-plugin-fiori-custom": "2.6.7",
    "@babel/eslint-parser": "7.14.7"
  },
  "scripts": {
    "start": "fiori run --open index.html",
    "start-local": "fiori run --config ./ui5-local.yaml --open index.html",
    "build": "ui5 build --all --config ./ui5.yaml --clean-dest --dest dist",
    "lint": "eslint ./",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open index.html",
    "int-test": "fiori run --open 'test/integration/opaTests.qunit.html'",
    "start-variants-management": "fiori run --open index.html",
    "unit-test": "fiori run --open 'test/unit/unitTests.qunit.html'"
  },
  "sapuxLayer": "CUSTOMER_BASE"
}
